{
  "version": 2,
  "builds": [
    {
      "src": "vercel-build.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".svelte-kit/output/static"
      }
    }
  ],
  "routes": [
    {
      "src": "/_app/immutable/(.*)",
      "dest": "/_app/immutable/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
